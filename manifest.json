


{
    "manifest_version": 2,
    "name": "LinkedIn AdBlocker",
    "description": "Blocks ads.",
    "version": "0.0.1",
    "author": "Ayush Verma",
    "browser_action" : {
        "default_title": "LinkedIn AdBlocker"
    },
    "permissions": [          
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/"
    ],
    "background":{                     
        "scripts": ["extension.js"]
    }
}